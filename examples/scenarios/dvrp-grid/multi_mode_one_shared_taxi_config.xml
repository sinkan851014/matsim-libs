<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v2.dtd">
<config>
	<module name="dvrp">
		<parameterset type="travelTimeMatrix">
			<param name="cellSize" value="10"/>
		</parameterset>
	</module>

	<module name="global">
		<param name="coordinateSystem" value="EPSG:3826" />
		<param name="numberOfThreads" value="1" />
	</module>

	<module name="multiModeDrt">
		<parameterset type="drt">
			<parameterset type="ExtensiveInsertionSearch"/>
			<param name="mode" value="drt_conventional"/>
			<param name="stopDuration" value="60"/>
			<param name="maxWaitTime" value="900"/>
			<param name="maxTravelTimeAlpha" value="1.3"/>
			<param name="maxTravelTimeBeta" value="600"/>
			<param name="rejectRequestIfMaxWaitOrTravelTimeViolated" value="false"/>
			<param name="changeStartLinkToLastLinkInSchedule" value="true"/>
			<parameterset type="drtfare">
				<!-- Basefare per trip (positive or zero value) -->
				<param name="basefare" value="0.0"/>
				<!-- Daily subscription fee (positive or zero value) -->
				<param name="dailySubscriptionFee" value="5.0"/>
				<!-- Daily subscription fee (positive or zero value) -->
				<param name="distanceFare_m" value="0.001"/>
			</parameterset>
		</parameterset>

		<parameterset type="drt">
			<parameterset type="ExtensiveInsertionSearch"/>
			<param name="mode" value="drt_autonomous"/>
			<param name="stopDuration" value="60"/>
			<param name="maxWaitTime" value="900"/>
			<param name="maxTravelTimeAlpha" value="1.3"/>
			<param name="maxTravelTimeBeta" value="600"/>
			<param name="rejectRequestIfMaxWaitOrTravelTimeViolated" value="false"/>
			<param name="changeStartLinkToLastLinkInSchedule" value="true"/>
			<parameterset type="drtfare">
				<!-- Basefare per trip (positive or zero value) -->
				<param name="basefare" value="0.0"/>
				<!-- Daily subscription fee (positive or zero value) -->
				<param name="dailySubscriptionFee" value="0.0"/>
				<!-- drt fare per meter (positive or zero value) -->
				<param name="distanceFare_m" value="0.0"/>
			</parameterset>
		</parameterset>

		<parameterset type="drt">
			<parameterset type="ExtensiveInsertionSearch"/>
			<param name="mode" value="drt_autonomousForCarSharing"/>
			<param name="stopDuration" value="60"/>
			<param name="maxWaitTime" value="900"/>
			<param name="maxTravelTimeAlpha" value="1.3"/>
			<param name="maxTravelTimeBeta" value="600"/>
			<param name="rejectRequestIfMaxWaitOrTravelTimeViolated" value="false"/>
			<param name="changeStartLinkToLastLinkInSchedule" value="true"/>
			<parameterset type="drtfare">
				<!-- Basefare per trip (positive or zero value) -->
				<param name="basefare" value="0.0"/>
				<!-- Daily subscription fee (positive or zero value) -->
				<param name="dailySubscriptionFee" value="0.0"/>
				<!-- drt fare per meter (positive or zero value) -->
				<param name="distanceFare_m" value="0.0"/>
			</parameterset>
		</parameterset>


		<parameterset type="drt">
			<parameterset type="ExtensiveInsertionSearch"/>
			<param name="mode" value="drt_autonomousForRideSharing"/>
			<param name="stopDuration" value="60"/>
			<param name="maxWaitTime" value="900"/>
			<param name="maxTravelTimeAlpha" value="1.3"/>
			<param name="maxTravelTimeBeta" value="600"/>
			<param name="rejectRequestIfMaxWaitOrTravelTimeViolated" value="false"/>
			<param name="changeStartLinkToLastLinkInSchedule" value="true"/>
				<parameterset type="drtfare">
					<!-- Basefare per trip (positive or zero value) -->
					<param name="basefare" value="0.0"/>
					<!-- Daily subscription fee (positive or zero value) -->
					<param name="dailySubscriptionFee" value="0.0"/>
					<!-- drt fare per meter (positive or zero value) -->
					<param name="distanceFare_m" value="0.0"/>
				</parameterset>
		</parameterset>

	</module>

	<module name="network">
		<param name="inputCRS" value="EPSG:3826" />
		<param name="inputNetworkFile" value="TaoyuanCity_boundary_3826_fixed.xml"/>
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="population_scenario.xml"/>
	</module>

	<module name="vehicles">
		<param name="vehiclesFile" value="one_shared_taxi_matsim_vehicles.xml"/>
	</module>

	<module name="qsim">
		<param name="simStarttimeInterpretation" value="onlyUseStarttime"/>
		<param name="insertingWaitingVehiclesBeforeDrivingVehicles" value="true"/>
		<param name="snapshotStyle" value="queue"/>
		<param name="mainMode" value="car,drt_autonomous,drt_autonomousForCarSharing,drt_autonomousForRideSharing" />
	</module>

	<module name="otfvis">
		<param name="coloringScheme" value="taxicab"/>
		<param name="drawNonMovingItems" value="true"/>
	</module>

	<module name="controler">
		<param name="outputDirectory" value="test/output/multi_mode_one_shared_taxi"/>
		<param name="overwriteFiles" value="deleteDirectoryIfExists"/>
		<param name="firstIteration" value="0"/>
		<param name="lastIteration" value="99"/>
	</module>

	<module name="planCalcScore">

		<param name="learningRate" value="1.0" />
		<param name="BrainExpBeta" value="2.0" />
		<param name="waiting" value="0" />
		<param name="lateArrival" value="0" />
		<param name="earlyDeparture" value="0" />
		<param name="performing" value="+270" />

		<parameterset type="activityParams" >
			<param name="activityType"            value="h" /> <!-- home -->
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="12:00:00" />
		</parameterset>

		<parameterset type="activityParams" >
			<param name="activityType"            value="w" /> <!-- work -->
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="08:00:00" />
			<param name="openingTime"     value="07:00:00" />
			<param name="latestStartTime" value="09:00:00" />
			<param name="earliestEndTime" value="" />
			<param name="closingTime"     value="18:00:00" />
		</parameterset>

		<param name="activityType_0" value="dummy"/>
		<param name="activityTypicalDuration_0" value="24:00:00"/>

		<param name="activityType_1" value="drt_conventional interaction"/>
		<param name="activityTypicalDuration_1" value="00:00:01"/>

		<param name="activityType_2" value="drt_autonomous interaction"/>
		<param name="activityTypicalDuration_2" value="00:00:01"/>

		<param name="activityType_3" value="drt_autonomousForCarSharing interaction"/>
		<param name="activityTypicalDuration_3" value="00:00:01"/>

		<param name="activityType_4" value="drt_autonomousForRideSharing interaction"/>
		<param name="activityTypicalDuration_4" value="00:00:01"/>
		<!-- [utils/hr] additional marginal utility of traveling.  normally negative.  this comes on top of the opportunity cost of time -->
		<param name="traveling_drt_conventional" value="0"/>
		<param name="traveling_drt_autonomous" value="0"/>
		<param name="traveling_drt_autonomousForCarSharing" value="0"/>
		<param name="traveling_drt_autonomousForRideSharing" value="0"/>

	<parameterset type="modeParams" >
		<param name="mode" value="car" />
		<param name="marginalUtilityOfTraveling_util_hr" value="-126.6" />
		<param name="monetaryDistanceRate" value="-0.00346" />
		<param name="constant" value="230.0" />
	</parameterset>

	<parameterset type="modeParams" >
		<param name="mode" value="motorcycle" />
		<param name="marginalUtilityOfTraveling_util_hr" value="-101.4" />
		<param name="monetaryDistanceRate" value="-0.00239" />
		<param name="constant" value="0" />
	</parameterset>

	<parameterset type="modeParams" >
		<param name="mode" value="pt" />
		<param name="marginalUtilityOfTraveling_util_hr" value="-111.6" />
		<param name="monetaryDistanceRate" value="-0.0007" />
		<param name="constant" value="0" />
	</parameterset>

	<parameterset type="modeParams" >
		<param name="mode" value="drt_autonomous" />
		<param name="marginalUtilityOfTraveling_util_hr" value="-63.3" />
		<param name="monetaryDistanceRate" value="-0.00359555" />
		<param name="constant" value="230" />
	</parameterset>

	<parameterset type="modeParams" >
		<param name="mode" value="drt_autonomousForCarSharing" />
		<param name="marginalUtilityOfTraveling_util_hr" value="-126.6" />
		<param name="monetaryDistanceRate" value="-0.00290355" />
		<param name="constant" value="230" />
	</parameterset>

	<parameterset type="modeParams" >
		<param name="mode" value="drt_autonomousForRideSharing" />
		<param name="marginalUtilityOfTraveling_util_hr" value="-126.6" />
		<param name="monetaryDistanceRate" value="-0.00784742" />
		<param name="constant" value="230" />
	</parameterset>
	</module>


	<module name="strategy">
		<param name="maxAgentPlanMemorySize" value="10" /> <!-- 0 means unlimited -->

		<param name="ModuleProbability_1" value="0.9" />
		<param name="Module_1" value="BestScore" />

		<param name="ModuleProbability_2" value="0.05" />
		<param name="Module_2" value="ReRoute" />

		<parameterset type="strategysettings" >
			<param name="strategyName" value="ChangeSingleTripMode" />
			<param name="weight" value="0.01" />
		</parameterset>

		<parameterset type="strategysettings" >
			<param name="disableAfterIteration" value="-1" />
			<param name="executionPath" value="null" />
			<param name="strategyName" value="SubtourModeChoice" />
			<param name="subpopulation" value="null" />
			<param name="weight" value="0.01" />
		</parameterset>

	</module>


	<module name="changeMode" >
		<param name="ignoreCarAvailability" value="true" />
		<param name="modes" value="car,pt,motorcycle,drt_autonomous,drt_autonomousForCarSharing,drt_autonomousForRideSharing" />
	</module>

	<module name="subtourModeChoice" >
		<param name="modes" value="car,pt,motorcycle,drt_autonomous,drt_autonomousForCarSharing,drt_autonomousForRideSharing" />
	</module>

	<module name="planscalcroute">
		<parameterset type="teleportedModeParameters">
			<param name="mode" value="motorcycle"/>
			<param name="teleportedModeSpeed" value="5.0"/>
			<param name="beelineDistanceFactor" value="1.3"/>
			<param name="teleportedModeFreespeedFactor" value="null" />
		</parameterset>

		<parameterset type="teleportedModeParameters">
			<param name="mode" value="walk"/>
			<param name="teleportedModeSpeed" value="1.5"/>
			<param name="beelineDistanceFactor" value="1.3"/>
			<param name="teleportedModeFreespeedFactor" value="null" />
		</parameterset>

		<parameterset type="teleportedModeParameters">
			<param name="mode" value="bike"/>
			<param name="teleportedModeSpeed" value="1.5"/>
			<param name="beelineDistanceFactor" value="1.3"/>
			<param name="teleportedModeFreespeedFactor" value="null" />
		</parameterset>

		<parameterset type="teleportedModeParameters">
			<param name="mode" value="pt"/>
			<param name="teleportedModeFreespeedFactor" value="3.0" />
			<param name="beelineDistanceFactor" value="null"/>
		</parameterset>
	</module>

</config>
